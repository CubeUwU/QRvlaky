<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity"
    android:background="@drawable/back"
    >

    <ImageButton
        android:id="@+id/scanner"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:srcCompat="@drawable/cityelefant"
        android:background="@drawable/corners"
        android:backgroundTint="#4b5666"
        android:scaleType="centerInside"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/button3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:layout_constraintWidth_percent="0.25"
        app:layout_constraintHeight_percent="0.11"
        />/>

    <ImageButton
        android:id="@+id/button3"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:srcCompat="@drawable/qr2b"
        android:background="@drawable/corners"
        android:backgroundTint="#4b5666"
        android:scaleType="centerInside"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/info"
        app:layout_constraintStart_toEndOf="@+id/scanner"
        app:layout_constraintVertical_bias="1.0"
        app:layout_constraintWidth_percent="0.25"
        app:layout_constraintHeight_percent="0.11"
         />
    <ImageButton
        android:id="@+id/info"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:srcCompat="@drawable/nerd"
        android:background="@drawable/corners"
        android:backgroundTint="#4b5666"
        android:scaleType="centerInside"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/neco"
        app:layout_constraintStart_toEndOf="@+id/button3"
        app:layout_constraintVertical_bias="1.0"
        app:layout_constraintWidth_percent="0.25"
        app:layout_constraintHeight_percent="0.11"
         />/>

    <ImageButton
        android:id="@+id/neco"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:srcCompat="@drawable/necop"
        android:background="@drawable/corners"
        android:backgroundTint="#4b5666"
        android:scaleType="centerInside"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/info"
        app:layout_constraintVertical_bias="1.0"
        app:layout_constraintWidth_percent="0.25"
        app:layout_constraintHeight_percent="0.11"
         />/>

</androidx.constraintlayout.widget.ConstraintLayout>